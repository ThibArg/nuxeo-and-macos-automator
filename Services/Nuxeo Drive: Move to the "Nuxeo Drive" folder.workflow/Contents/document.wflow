				<key>ActionName</key>
				<string>Run AppleScript</string>
				<key>ActionParameters</key>
				<dict>
					<key>source</key>
					<string>on run {input, parameters}	set kNUXEO_DRIVE_FOLDER_NAME to "Nuxeo Drive"	set kNUXEO_DRIVE_POSIX_TEXT to ((POSIX path of (path to home folder)) as text) &amp; kNUXEO_DRIVE_FOLDER_NAME &amp; "/"	set nxDriveFolderExists to false	set destFolder to ""		tell application "Finder"		set nxDriveFolderExists to exists (kNUXEO_DRIVE_POSIX_TEXT as POSIX file)	end tell		if (not nxDriveFolderExists) then		display dialog "Nuxeo Drive is not installed on your computer." buttons {"Ok"}	else				try			set nuxeoDrivePathAsAlias to POSIX file kNUXEO_DRIVE_POSIX_TEXT as alias			set destFolder to choose folder with prompt "Select the destination folder" default location nuxeoDrivePathAsAlias			set destFolderPosix to (POSIX path of destFolder)			if ((offset of kNUXEO_DRIVE_POSIX_TEXT in destFolderPosix) â‰  1) then				display dialog "Please, select a folder inside the \"" &amp; kNUXEO_DRIVE_FOLDER_NAME &amp; "\" folder." buttons {"Ok"}				set destFolder to ""			else if (kNUXEO_DRIVE_POSIX_TEXT = destFolderPosix) then				display dialog "Please, select a sub-folder inside the \"" &amp; kNUXEO_DRIVE_FOLDER_NAME &amp; "\" folder." buttons {"Ok"}				set destFolder to ""			else				tell application "Finder"					move input to destFolder with replacing				end tell			end if		on error theError			display dialog theError as text			set destFolder to ""		end try	end if	return destFolderend run</string>
